{
"regex": {
    "DELIMIT":    "[,;{}()]",
    "INTEGER":    "-?[0-9]+",
    "FLOAT":      "-?[0-9]+[.][0-9]+",
    "IDENTIFIER": "[a-zA-Z_][a-zA-Z0-9_]",
    "STRING":     "\"[^\"]*\"|'[^']*'"
    },
"keywords":  ["if", "else", "switch", "case", "default", "break", "int", "float", "char", "double", "long", "for", "while", "do", "void", "goto", "auto", "signed", "const", "extern", "register", "unsigned", "return", "continue", "enum", "sizeof", "struct", "typedef", "union", "volatile"],
"operators": {
    "arithmetic": ["-","+","*","/","%"],
    "inc_dec":    ["++", "--"],
    "assignment": ["=","+=", "-=", "*=", "/=", "%="],
    "relational": ["==", ">", "<", "!=", ">=", "<="],
    "logical":    ["&&", "||", "!"]
    }
}